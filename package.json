  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.17.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --respawn ./src/index.ts",
    "build": "cross-env NODE_ENV=development tsc --project ./tsconfig.json",
    "test": "jest --passWithNoTests",
    "lint": "yarn lint:type && yarn lint:eslint && yarn lint:prettier",
    "lint:type": "tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,ts}'",
    "lint:prettier": "prettier --check '**/*.{js,ts,md,yml,yaml,html,json}'"
  },
  "dependencies": {
    "apollo-server": "3.0.0",
    "graphql": "15.5.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "14.17.x",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "cross-env": "7.0.3",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "jest": "^27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5"
  }
}
